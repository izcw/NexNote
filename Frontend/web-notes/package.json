{
  "name": "ThinKing",
  "version": "1.0.0",
  "private": true,
  "description": "这是一个笔记软件",
  "author": "Izcw",
  "main": "electron.js",
  "scripts": {
    "dev": "vite --host --port 7436",
    "build": "vite build",
    "preview": "vite preview",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "pack": "electron-packager . ThinKing --platform=win32 --arch=x64 --out=build --icon=src/assets/logo.ico --overwrite",
    "make-installer": "electron-builder"
  },
  "dependencies": {
    "@tiptap/extension-bubble-menu": "^2.10.3",
    "@tiptap/extension-character-count": "^2.10.3",
    "@tiptap/extension-color": "^2.10.3",
    "@tiptap/extension-document": "^2.10.3",
    "@tiptap/extension-dropcursor": "^2.10.3",
    "@tiptap/extension-heading": "^2.10.3",
    "@tiptap/extension-history": "^2.10.3",
    "@tiptap/extension-mention": "^2.10.3",
    "@tiptap/extension-paragraph": "^2.10.3",
    "@tiptap/extension-placeholder": "^2.10.3",
    "@tiptap/extension-text": "^2.10.3",
    "@tiptap/extension-text-style": "^2.10.3",
    "@tiptap/pm": "^2.10.3",
    "@tiptap/suggestion": "^2.10.3",
    "@tiptap/vue-3": "^2.10.3",
    "axios": "^1.7.7",
    "dayjs": "^1.11.13",
    "element-plus": "^2.8.7",
    "lodash": "^4.17.21",
    "pinia": "^2.2.4",
    "relation-graph-vue3": "^2.2.8",
    "screenfull": "^6.0.2",
    "v-click-outside": "^3.2.0",
    "vue": "^3.5.11",
    "vue-router": "^4.4.5",
    "vue3-colorpicker": "^2.3.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.5.0",
    "@electron-forge/maker-deb": "^7.5.0",
    "@electron-forge/maker-rpm": "^7.5.0",
    "@electron-forge/maker-squirrel": "^7.5.0",
    "@electron-forge/maker-zip": "^7.5.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.5.0",
    "@electron-forge/plugin-fuses": "^7.5.0",
    "@vicons/fa": "^0.12.0",
    "@vicons/fluent": "^0.12.0",
    "@vicons/utils": "^0.1.4",
    "@vitejs/plugin-vue": "^5.1.4",
    "electron": "^33.0.2",
    "electron-builder": "^25.1.8",
    "electron-installer-windows": "^3.0.0",
    "electron-packager": "^17.1.2",
    "naive-ui": "^2.40.1",
    "sass-embedded": "^1.79.5",
    "vite": "^5.4.8"
  },
  "build": {
    "appId": "com.yourapp.id",
    "productName": "ThinKing",
    "files": [
      "build/**/*",
      "src/**/*",
      "electron.js",
      "preload.js"
    ],
    "win": {
      "target": [
        "squirrel"
      ]
    },
    "mac": {
      "target": [
        "dmg"
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ]
    }
  }
}
